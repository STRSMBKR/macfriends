# -*- encoding: utf-8 -*-

Gem::Specification.new do |s|
  s.name = "nibbler"
  s.version = "1.3.0"

  s.required_rubygems_version = Gem::Requirement.new(">= 0") if s.respond_to? :required_rubygems_version=
  s.authors = ["Mislav Marohni\u{107}"]
  s.date = "2012-01-17"
  s.description = "Nibbler is a super simple and powerful declarative generic scraper written in under 70 lines of code."
  s.email = "mislav.marohnic@gmail.com"
  s.homepage = "https://github.com/mislav/nibbler"
  s.require_paths = ["lib"]
  s.rubygems_version = "2.0.3"
  s.summary = "A cute HTML scraper / data extraction tool"
end
